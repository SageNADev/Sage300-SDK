diff --git a/Areas/Shared/Scripts/Sage.CA.SBS.ERP.Sage300.Common.SessionTimeOut.js b/Areas/Shared/Scripts/Sage.CA.SBS.ERP.Sage300.Common.SessionTimeOut.js
