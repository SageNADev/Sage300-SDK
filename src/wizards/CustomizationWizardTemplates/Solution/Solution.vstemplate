  <TemplateData>
    <Name>Sage 300 UI Customization Wizard</Name>
    <Description>A wizard to quickly scaffold a Sage 300 MVC Customization application</Description>
    <ProjectType>CSharp</ProjectType>
    <ProjectSubType>
    </ProjectSubType>
    <SortOrder>1000</SortOrder>
    <CreateNewFolder>true</CreateNewFolder>
    <DefaultName>Sage300UICustomization</DefaultName>
