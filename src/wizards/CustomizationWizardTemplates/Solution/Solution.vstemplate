    <TemplateContent>
        <ProjectCollection />
    </TemplateContent>
  <WizardExtension>
    <Assembly>Sage300UICustomizationWizard, Version=1.0.0.0, Culture=Neutral, PublicKeyToken=0627e4cb02c68557</Assembly>
    <FullClassName>Sage300UICustomizationWizard.Sage300UICustomizationUserInterface</FullClassName>
  </WizardExtension>
</VSTemplate>

       