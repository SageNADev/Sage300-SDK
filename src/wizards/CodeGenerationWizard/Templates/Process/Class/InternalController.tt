<#
	if (enumsCount > 0)
	{
#>
using <#=companyNamespace#>.<#=moduleId#>.Models.Enums.Process;
<#
	}	
#>
using <#=companyNamespace#>.<#=moduleId#>.Resources.Process;
using <#=companyNamespace#>.<#=webModuleNamespace#>.Models.Process;
